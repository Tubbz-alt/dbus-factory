<node>
  <interface name="org.freedesktop.Secret.Prompt">
    <method name="Prompt">
      <arg name="window-id" type="s" direction="in"/>
    </method>
    <method name="Dismiss"></method>
    <signal name="Completed">
      <arg name="dismissed" type="b"/>
      <arg name="result"    type="v"/>
    </signal>
  </interface>
</node>
