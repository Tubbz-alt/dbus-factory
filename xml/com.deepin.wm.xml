<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<!-- GDBus 2.46.2 --><node>
  <interface name="com.deepin.wm">
    <method name="PerformAction">
      <arg direction="in" name="type" type="i"/>
    </method>
    <method name="ToggleDebug"></method>
    <method name="PresentWindows">
      <arg direction="in" name="xids" type="au"/>
    </method>
    <method name="RequestHideWindows"></method>
    <method name="CancelHideWindows"></method>
    <method name="GetBackgroundColorInformation">
      <arg direction="in" name="monitor" type="i"/>
      <arg direction="in" name="reference_x" type="i"/>
      <arg direction="in" name="reference_y" type="i"/>
      <arg direction="in" name="reference_width" type="i"/>
      <arg direction="in" name="reference_height" type="i"/>
      <arg direction="out" name="result" type="(ddddd)"/>
    </method>
    <signal name="BackgroundChanged"></signal>
  </interface>
</node>
