      <arg type="o" direction="in"></arg>
    </method>
    <method name="FeedAuthorizeService">
      <arg type="s" direction="in"></arg>
    </method>
    <method name="FeedPinCode">
      <arg type="s" direction="in"></arg>
    </method>
    <method name="GetDevices">
      <arg type="s" direction="out"></arg>
